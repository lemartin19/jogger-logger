{
  "name": "jogger-logger-server",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm install -g typescript && tsc --build",
    "clean": "rm -rf dist server.js && tsc --build --clean",
    "dev": "npx ts-node ./src/index.ts",
    "ts": "tsc --noEmit -w",
    "format": "prettier --write src/",
    "start": "node src/index.js"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.0.1",
    "@types/node": "^20.10.6"
  }
}
